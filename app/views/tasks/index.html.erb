<div class='signinPage'>
  <div class="container">
    <div class="row">
      <div class="mx-auto">
        <div class="text">
          <h1>タスク一覧</h1>
        <table>
            <thead>
                <tr>
              <th>タスク名</th>
                </tr>
            </thead>
                <tbody>
                  <% @tasks.each do |task| %>
                <tr>
                  <td><%= task.title %></td>
                </tr>
                  <% end %>
                </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>